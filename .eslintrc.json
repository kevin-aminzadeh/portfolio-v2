{
  "env": {
    "es6": true,
    "jest": true,
    "browser": true
  },
  "plugins": ["@typescript-eslint"],
  "extends": [
    "airbnb",
    "airbnb-typescript",
    "prettier",
    "next",
    "next/core-web-vitals"
  ],
  "parserOptions": {
    "project": ["./tsconfig.eslint.json"]
  },
  "rules": {
    "react/react-in-jsx-scope": "off"
  },
  "globals": {
    "React": "writable"
  }
}
